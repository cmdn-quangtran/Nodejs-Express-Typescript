  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "codegen": "rm -rf src/generated/api && openapi2aspida -i ./openapi.yaml -o src/generated/api",
    "build": "npm run codegen && tsx build.ts",
    "config:local": "cp ./.env.local .env",
