    "express": "^4.19.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-openapi-validator": "^5.2.0",
    "inversify": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "openapi2aspida": "^0.23.2",
    "pino": "^9.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
